{
  "security.workspace.trust.untrustedFiles": "open",
  "partialDiff.enableTelemetry": false,
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false,
  "customWindowZoom.zoomLevelChangeValue": 0.1,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "*//.git": true,
    "*//.svn": true,
    "*//.hg": true,
    "*//CVS": true,
    "*//.DS_Store": true,
    "*//Thumbs.db": true,
    "*//__pycache__": true
  },
  "terminal.explorerKind": "external",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.lineHeight": 1.1,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#211e26",
    "editor.lineHighlightBorder": "#49264445",
    "editor.selectionBackground": "#77276b45",
    "editor.selectionHighlightBackground": "#135564"
  },
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "diffEditor.wordWrap": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "block-outline",
  "editor.guides.bracketPairs": false,
  "editor.guides.indentation": true,
  "editor.inlayHints.enabled": "off",
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "on",
  "files.associations": {
    "*.tf": "tf",
    "*.tfvars": "tfvars",
    "vimrc": "plaintext"
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "json.maxItemsComputed": 50000,
  "update.showReleaseNotes": false,
  "go.formatTool": "gofmt",
  "go.lintOnSave": "package",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.formatOnSave": true
  },
  "gopls": {
    "ui.semanticTokens": true,
    "formatting.gofumpt": false
  },
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": [
    "--line-length=100"
  ],
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=100"
  ],
  "python.terminal.activateEnvInCurrentTerminal": true,
  "gitlens.hovers.currentLine.over": "line",
  "shellcheck.enable": true,
  "shellcheck.enableQuickFix": true,
  "shellcheck.run": "onType",
  "shellcheck.executablePath": "/opt/homebrew/bin/shellcheck",
  "shellcheck.exclude": [],
  "shellcheck.customArgs": [],
  "shellcheck.ignorePatterns": {
    "*///.xonshrc": true,
    "*///.xsh": true,
    "*///.zsh": true,
    "*///.zshrc": true,
    "*//zshrc": true,
    "*///.zprofile": true,
    "*//zprofile": true,
    "*///.zlogin": true,
    "*//zlogin": true,
    "*///.zlogout": true,
    "*//zlogout": true,
    "*///.zshenv": true,
    "*//zshenv": true,
    "*///.zsh-theme": true
  },
  "shellcheck.ignoreFileSchemes": [
    "git",
    "gitfs"
  ],
  "[shellscript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.shellcheck": true
    },
    "files.eol": "\n"
  },
  "svg.preview.mode": "svg",
  "explorer.confirmDragAndDrop": false,
  "hediet.vscode-drawio.resizeImages": null,
  "workbench.colorTheme": "Tokyo Night",
  "[markdown]": {
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "tabnine.experimentalAutoImports": true,
  "editor.largeFileOptimizations": false,
  "python.languageServer": "Pylance",
  "window.zoomLevel": 0.9,
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3"
}
